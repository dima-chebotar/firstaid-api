{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "gpt": {
      "type": "library",
      "root": "libs/gpt",
      "entryFile": "index",
      "sourceRoot": "libs/gpt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/gpt/tsconfig.lib.json"
      }
    },
    "playht": {
      "type": "library",
      "root": "libs/playht",
      "entryFile": "index",
      "sourceRoot": "libs/playht/src",
      "compilerOptions": {
        "tsConfigPath": "libs/playht/tsconfig.lib.json"
      }
    },
    "pdf-parser": {
      "type": "library",
      "root": "libs/pdf-parser",
      "entryFile": "index",
      "sourceRoot": "libs/pdf-parser/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pdf-parser/tsconfig.lib.json"
      }
    },
    "mail": {
      "type": "library",
      "root": "libs/mail",
      "entryFile": "index",
      "sourceRoot": "libs/mail/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mail/tsconfig.lib.json"
      }
    }
  }
}